            </geometry>
        </visual>
      </link>

      <joint name = "left_knee_to_left_shin" type = "continuous">
      <parent link = "left_knee"/>
      <child link = "left_shin"/>
      <origin xyz = "0 0 -0.25"/>
      <axis xyz="0 1 0"/>
      </joint>

      <link name = "left_ankle">
        <visual>
        <geometry>
            <sphere radius = "0.1"/>
          </geometry>
          <material name = "white"/>
        </visual>
      </link>

      <joint name = "left_shin_to_left_ankle" type = "continuous">
      <parent link = "left_shin"/>
      <child link = "left_ankle"/>
      <origin xyz = "0 0 -0.2"/>
      <axis xyz="0 1 0"/>
      </joint>

       <link name = "left_foot">
        <visual>
        <geometry>
            <box size = "0.3 0.1 0.15"/>
          </geometry>
        </visual>
      </link>

      <joint name = "left_ankle_to_left_foot" type = "continuous">
      <parent link = "left_ankle"/>
      <child link = "left_foot"/>
      <origin xyz = "0.15 0 0"/>
      <axis xyz="0 1 0"/>
      </joint>

      <link name = "right_hip">
      <visual>
          <geometry>
            <sphere radius = "0.1"/>
          </geometry>
          <material name = "white"/>
        </visual>
      </link>

      <joint name = "base_to_right_hip" type = "continuous">
      <parent link = "base_link"/>
      <child link = "right_hip"/>
      <origin xyz = "0 0.15 1.1"/>
      <axis xyz="0 1 0"/>
      </joint>

      <link name = "right_thigh">
        <visual>
            <geometry>
                <box size = "0.2 0.1 0.4"/>
            </geometry>
        </visual>
      </link>

      <joint name = "base_to_right_thigh" type = "continuous">
      <parent link = "right_hip"/>
      <child link = "right_thigh"/>
      <origin xyz = "0 0 -0.225"/>
      <axis xyz="0 1 0"/>
