            <box size = "0.3 0.1 0.15"/>
          </geometry>
        </visual>
      </link>

      <joint name = "right_ankle_to_right_foot" type = "fixed">
      <parent link = "right_ankle"/>
      <child link = "right_foot"/>
      <origin xyz = "0.15 0 0"/>
      </joint>

    </robot>