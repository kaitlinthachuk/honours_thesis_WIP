<?xml version="1.0"?>
    <robot name="legs">

    <material name="blue">
        <color rgba="0 0 0.8 1"/>
    </material>

    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

      <link name="base_link">
        <visual>
          <geometry>
            <box size = "0.2 0.4 0.8"/>
          </geometry>
          <origin  xyz = "0 0 0"/>
          <material name = "blue"/>
        </visual>
        <collision>
            <geometry>
                <box size = "0.2 0.4 0.8"/>
            </geometry>
            <origin  xyz = "0 0 0"/>
        </collision>
        <inertial>
         <mass value="10"/>
         <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
       </inertial>
      </link>

<!-- beginning of left leg-->
      <link name = "left_hip">
      <visual>
          <geometry>
            <sphere radius = "0.1"/>
          </geometry>
          <material name = "white"/>
        </visual>
        <inertial>
         <mass value="0.1"/>
         <inertia ixx="0.0004" ixy="0.0" ixz="0.0" iyy="0.0004" iyz="0.0" izz="0.0004"/>
       </inertial>
      </link>

      <joint name = "base_to_left_hip" type = "fixed">
      <parent link = "base_link"/>
      <child link = "left_hip"/>
      <origin xyz = "0 -0.1 -0.4"/>
      </joint>

      <link name = "left_thigh">
        <visual>
            <geometry>
                <box size = "0.2 0.1 0.4"/>
            </geometry>
            <material name = "black"/>
        </visual>
        <collision>
            <geometry>
               <box size = "0.2 0.1 0.4"/>
            </geometry>
        </collision>
        <inertial>
         <mass value="8"/>
         <inertia ixx="0.113333" ixy="0.0" ixz="0.0" iyy="0.133333" iyz="0.0" izz="0.033333"/>
       </inertial>
      </link>


      <joint name="left_hip_to_left_thigh" type="revolute">
        <axis xyz="0 1 0"/>
        <limit effort="1000.0" lower="-0.785398" upper="0.785398" velocity="0.5"/>
        <origin rpy="0 0 0" xyz="0 0 -0.2"/>
        <parent link="left_hip"/>
        <child link="left_thigh"/>
      </joint>

      <link name = "left_knee">
