          <material name = "white"/>
        </visual>
      </link>

      <joint name = "right_thigh_to_right_knee" type = "continuous">
      <parent link = "right_thigh"/>
      <child link = "right_knee"/>
      <origin xyz = "0 0 -0.2"/>
      <axis xyz="0 1 0"/>
      </joint>

      <link name = "right_shin">
        <visual>
            <geometry>
                <box size = "0.2 0.1 0.4"/>
            </geometry>
        </visual>
      </link>

      <joint name = "right_knee_to_right_shin" type = "continuous">
      <parent link = "right_knee"/>
      <child link = "right_shin"/>
      <origin xyz = "0 0 -0.25"/>
      <axis xyz="0 1 0"/>
      </joint>

      <link name = "right_ankle">
        <visual>
        <geometry>
            <sphere radius = "0.1"/>
          </geometry>
          <material name = "white"/>
        </visual>
      </link>

      <joint name = "right_shin_to_right_ankle" type = "continuous">
