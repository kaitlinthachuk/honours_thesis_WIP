<?xml version="1.0"?>
    <robot name="legs">

    <material name="blue">
        <color rgba="0 0 0.8 1"/>
    </material>

    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

      <link name="base_link">
        <visual>
          <geometry>
            <box size = "0.2 0.4 0.8"/>
          </geometry>
          <origin  xyz = "0 0 1.5"/>
          <material name = "blue"/>
        </visual>
      </link>

      <link name = "left_hip">
      <visual>
          <geometry>
            <sphere radius = "0.1"/>
          </geometry>
          <material name = "white"/>
        </visual>
      </link>

      <joint name = "base_to_left_hip" type = "continuous">
      <parent link = "base_link"/>
      <child link = "left_hip"/>
      <origin xyz = "0 -0.15 1.1"/>
      <axis xyz="0 1 0"/>
      </joint>

      <link name = "left_thigh">
        <visual>
            <geometry>
                <box size = "0.2 0.1 0.4"/>
            </geometry>
        </visual>
      </link>

      <joint name = "base_to_left_thigh" type = "continuous">
      <parent link = "left_hip"/>
      <child link = "left_thigh"/>
      <origin xyz = "0 0 -0.225"/>
       <axis xyz="0 1 0"/>
      </joint>

      <link name = "left_knee">
        <visual>
        <geometry>
            <sphere radius = "0.1"/>
          </geometry>
          <material name = "white"/>
        </visual>
      </link>

      <joint name = "left_thigh_to_left_knee" type = "continuous">
      <parent link = "left_thigh"/>
      <child link = "left_knee"/>
      <origin xyz = "0 0 -0.2"/>
      <axis xyz="0 1 0"/>
      </joint>

      <link name = "left_shin">
